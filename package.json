{
  "name": "tfjs-electron-app",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "start": "tsc && electron .",
    "build": "tsc && electron-builder --dir",
    "dist": "tsc && electron-builder"
  },
  "build": {
    "appId": "com.example.tfjs-electron-app",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "index.html"
    ],
    "directories": {
      "output": "build"
    },
    "asar": true
  }
}